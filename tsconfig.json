{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "paths": {
      "@app/kafka": [
        "libs/kafka/src"
      ],
      "@app/kafka/*": [
        "libs/kafka/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/mission-user": [
        "libs/mission-user/src"
      ],
      "@app/mission-user/*": [
        "libs/mission-user/src/*"
      ],
      "@app/mission": [
        "libs/mission/src"
      ],
      "@app/mission/*": [
        "libs/mission/src/*"
      ],
      "@app/mission-user-log": [
        "libs/mission-user-log/src"
      ],
      "@app/mission-user-log/*": [
        "libs/mission-user-log/src/*"
      ],
      "@app/mysql": [
        "libs/mysql/src"
      ],
      "@app/mysql/*": [
        "libs/mysql/src/*"
      ],
      "@app/user": [
        "libs/user/src"
      ],
      "@app/user/*": [
        "libs/user/src/*"
      ],
      "@app/external-user": [
        "libs/external-user/src"
      ],
      "@app/external-user/*": [
        "libs/external-user/src/*"
      ],
      "@app/campaign": [
        "libs/campaign/src"
      ],
      "@app/campaign/*": [
        "libs/campaign/src/*"
      ],
      "@app/affiliate-internal": [
        "libs/affiliate-internal/src"
      ],
      "@app/affiliate-internal/*": [
        "libs/affiliate-internal/src/*"
      ],
      "@app/reward-rule": [
        "libs/reward-rule/src"
      ],
      "@app/reward-rule/*": [
        "libs/reward-rule/src/*"
      ],
      "@app/admin-action-log": [
        "libs/admin-action-log/src"
      ],
      "@app/admin-action-log/*": [
        "libs/admin-action-log/src/*"
      ],
      "@lib/external-balance": [
        "libs/external-balance/src"
      ],
      "@lib/external-balance/*": [
        "libs/external-balance/src/*"
      ]
    }
  }
}