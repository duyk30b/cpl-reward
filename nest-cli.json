{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/reward/src",
  "monorepo": true,
  "root": "apps/reward",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/reward/tsconfig.app.json"
  },
  "projects": {
    "reward": {
      "type": "application",
      "root": "apps/reward",
      "entryFile": "main",
      "sourceRoot": "apps/reward/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reward/tsconfig.app.json"
      }
    },
    "common": {
      "type": "application",
      "root": "apps/common",
      "entryFile": "main",
      "sourceRoot": "apps/common/src",
      "compilerOptions": {
        "tsConfigPath": "apps/common/tsconfig.app.json"
      }
    },
    "campaign01": {
      "type": "application",
      "root": "apps/campaign01",
      "entryFile": "main",
      "sourceRoot": "apps/campaign01/src",
      "compilerOptions": {
        "tsConfigPath": "apps/campaign01/tsconfig.app.json"
      }
    },
    "campaign02": {
      "type": "application",
      "root": "apps/campaign02",
      "entryFile": "main",
      "sourceRoot": "apps/campaign02/src",
      "compilerOptions": {
        "tsConfigPath": "apps/campaign02/tsconfig.app.json"
      }
    }
  }
}