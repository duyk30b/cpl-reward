{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/reward/src",
  "monorepo": true,
  "root": "apps/reward",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/reward/tsconfig.app.json"
  },
  "projects": {
    "reward": {
      "type": "application",
      "root": "apps/reward",
      "entryFile": "main",
      "sourceRoot": "apps/reward/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reward/tsconfig.app.json"
      }
    },
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "mission-user": {
      "type": "library",
      "root": "libs/mission-user",
      "entryFile": "index",
      "sourceRoot": "libs/mission-user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mission-user/tsconfig.lib.json"
      }
    },
    "mission": {
      "type": "library",
      "root": "libs/mission",
      "entryFile": "index",
      "sourceRoot": "libs/mission/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mission/tsconfig.lib.json"
      }
    },
    "mission-user-log": {
      "type": "library",
      "root": "libs/mission-user-log",
      "entryFile": "index",
      "sourceRoot": "libs/mission-user-log/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mission-user-log/tsconfig.lib.json"
      }
    },
    "mysql": {
      "type": "library",
      "root": "libs/mysql",
      "entryFile": "index",
      "sourceRoot": "libs/mysql/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mysql/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "external-user": {
      "type": "library",
      "root": "libs/external-user",
      "entryFile": "index",
      "sourceRoot": "libs/external-user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/external-user/tsconfig.lib.json"
      }
    },
    "missions": {
      "type": "application",
      "root": "apps/missions",
      "entryFile": "main",
      "sourceRoot": "apps/missions/src",
      "compilerOptions": {
        "tsConfigPath": "apps/missions/tsconfig.app.json"
      }
    },
    "campaign": {
      "type": "library",
      "root": "libs/campaign",
      "entryFile": "index",
      "sourceRoot": "libs/campaign/src",
      "compilerOptions": {
        "tsConfigPath": "libs/campaign/tsconfig.lib.json"
      }
    },
    "affiliate-internal": {
      "type": "library",
      "root": "libs/affiliate-internal",
      "entryFile": "index",
      "sourceRoot": "libs/affiliate-internal/src",
      "compilerOptions": {
        "assets": [
          "**/*.proto",
          "*.proto"
        ],
        "watchAssets": true,
        "tsConfigPath": "libs/affiliate-internal/tsconfig.lib.json"
      }
    },
    "reward-rule": {
      "type": "library",
      "root": "libs/reward-rule",
      "entryFile": "index",
      "sourceRoot": "libs/reward-rule/src",
      "compilerOptions": {
        "tsConfigPath": "libs/reward-rule/tsconfig.lib.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "assets": ["**/*.proto"],
        "watchAssets": true,
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "admin-action-log": {
      "type": "library",
      "root": "libs/admin-action-log",
      "entryFile": "index",
      "sourceRoot": "libs/admin-action-log/src",
      "compilerOptions": {
        "tsConfigPath": "libs/admin-action-log/tsconfig.lib.json"
      }
    }
  }
}
